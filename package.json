{
  "private": true,
  "sideEffects": false,
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "scripts": {
    "build": "remix build",
    "dev": "remix build && run-p \"dev:*\"",
    "dev:node": "cross-env NODE_ENV=development nodemon --require dotenv/config  --require esbuild-register ./server.js --watch ./server.js",
    "dev:remix": "remix watch",
    "eslint": "eslint .",
    "prettier": "prettier --ignore-path .eslintignore .",
    "prettier:write": "pnpm run prettier --write",
    "prettier:check": "pnpm run prettier --check",
    "start": "cross-env NODE_ENV=production node --require esbuild-register ./server.js",
    "typecheck": "tsc -b",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "pnpx chromatic",
    "generate-dmmf": "node --require esbuild-register prisma/dmmf.ts",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"yarn build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on http://127.0.0.1:6006 && yarn test-storybook --url http://127.0.0.1:6006\""
  },
  "dependencies": {
    "@prisma/client": "^4.8.0",
    "@remix-run/express": "1.10.0-pre.5",
    "@remix-run/node": "1.10.0-pre.5",
    "@remix-run/react": "1.10.0-pre.5",
    "@remix-run/router": "^1.2.1",
    "@remix-run/server-runtime": "^1.9.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "isbot": "^3.6.5",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "1.10.0-pre.5",
    "@remix-run/eslint-config": "1.10.0-pre.5",
    "@storybook/addon-a11y": "^7.0.0-beta.15",
    "@storybook/addon-essentials": "^7.0.0-beta.15",
    "@storybook/addon-interactions": "^7.0.0-beta.15",
    "@storybook/addon-links": "^7.0.0-beta.15",
    "@storybook/blocks": "^7.0.0-beta.15",
    "@storybook/jest": "^0.0.11-next.0",
    "@storybook/react": "^7.0.0-beta.15",
    "@storybook/react-vite": "^7.0.0-beta.15",
    "@storybook/test-runner": "^0.10.0-next.4",
    "@storybook/testing-library": "^0.0.13",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.15",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-react": "^3.0.0",
    "chromatic": "^6.14.0",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.16.12",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.27.0",
    "eslint-plugin-storybook": "^0.6.8",
    "http-server": "^14.1.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.29.1",
    "prettier": "^2.8.1",
    "prisma": "^4.8.0",
    "react-router-dom": "6.6.1",
    "storybook": "^7.0.0-beta.15",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0",
    "vite": "^4.0.3",
    "wait-on": "^7.0.1"
  },
  "engines": {
    "node": ">=18"
  }
}
