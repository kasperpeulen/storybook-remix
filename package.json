{
  "private": true,
  "sideEffects": false,
  "prisma": {
    "seed": "pnpm run seed"
  },
  "scripts": {
    "build": "remix build",
    "dev": "remix build && run-p \"dev:*\"",
    "dev:node": "cross-env NODE_ENV=development nodemon --require dotenv/config  --require esbuild-register ./server.ts --watch ./server.ts",
    "dev:remix": "remix watch",
    "eslint": "eslint .",
    "prettier": "prettier --ignore-path .eslintignore .",
    "prettier:write": "pnpm run prettier --write",
    "prettier:check": "pnpm run prettier --check",
    "start": "cross-env NODE_ENV=production node --require esbuild-register ./server.ts",
    "typecheck": "tsc -b",
    "storybook:dev": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build",
    "storybook:serve": "pnpm run storybook:build && npx http-server storybook-static --port 6007",
    "storybook:serve:coverage": "VITE_COVERAGE=true pnpm run storybook:serve",
    "chromatic": "chromatic",
    "build-storybook": "storybook build",
    "generate-dmmf": "node --require esbuild-register prisma/dmmf.ts",
    "seed": "node --require esbuild-register prisma/seed.ts && pnpm run generate-dmmf",
    "test": "jest",
    "test:coverage": "TARGET_URL=http://127.0.0.1:6007 pnpm run test --coverage",
    "test:ci": "concurrently -k -s first -n \"sb,test\" -c \"cyan,blue\" \"pnpm run storybook:serve:coverage --silent\" \"wait-on http://127.0.0.1:6007 && pnpm run test:coverage\"",
    "ci": "pnpm i && pnpm run prettier:write && pnpm run eslint && pnpm run typecheck"
  },
  "dependencies": {
    "@isbl/bcryptjs": "3.0.0-beta.1",
    "@prisma/client": "^4.8.0",
    "@remix-run/express": "1.10.0-pre.5",
    "@remix-run/node": "1.10.0-pre.5",
    "@remix-run/react": "1.10.0-pre.5",
    "@remix-run/router": "^1.2.1",
    "@remix-run/server-runtime": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-signature": "^1.2.0",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "isbot": "^3.6.5",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@remix-run/dev": "1.10.0-pre.5",
    "@remix-run/eslint-config": "1.10.0-pre.5",
    "@storybook/addon-a11y": "^7.0.0-beta.15",
    "@storybook/addon-essentials": "^7.0.0-beta.15",
    "@storybook/addon-interactions": "^7.0.0-beta.15",
    "@storybook/addon-links": "^7.0.0-beta.15",
    "@storybook/blocks": "^7.0.0-beta.15",
    "@storybook/csf": "^0.0.2-next.8",
    "@storybook/jest": "^0.0.11-next.0",
    "@storybook/react": "^7.0.0-beta.15",
    "@storybook/react-vite": "^7.0.0-beta.15",
    "@storybook/test-runner": "^0.10.0-next.4",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/dom": "^8.19.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-signature": "^1.1.0",
    "@types/express": "^4.17.15",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-react": "^3.0.0",
    "chromatic": "^6.14.0",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.16.12",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.27.0",
    "eslint-plugin-storybook": "^0.6.8",
    "http-server": "^14.1.1",
    "jest": "^29.3.1",
    "jest-mock": "^29.3.1",
    "jest-playwright-istanbul": "^0.5.16",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.29.1",
    "prettier": "^2.8.1",
    "prettier-plugin-prisma": "^4.8.0",
    "prisma": "^4.8.0",
    "react-router-dom": "6.6.1",
    "storybook": "^7.0.0-beta.15",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0",
    "vite": "^4.0.3",
    "vite-plugin-istanbul": "^3.0.4",
    "wait-on": "^7.0.1"
  },
  "engines": {
    "node": ">=18"
  },
  "prettier": {
    "printWidth": 120,
    "plugins": [
      "prettier-plugin-prisma"
    ]
  }
}
